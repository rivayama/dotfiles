" Color scheme
colorscheme jellybeans

set columns=180
set lines=58
set cmdheight=1
set noswapfile
set nobackup
set guifont=Monaco:h11
set guioptions-=r
set guioptions-=L
set textwidth=0
set undodir=/tmp
set showtabline=2
set guioptions-=e

highlight Normal guibg=#232323
highlight NonText guifg=#585858 guibg=#2A2A2A
highlight SpecialKey guifg=#585858 guibg=#2A2A2A
