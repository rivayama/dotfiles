colorscheme Jellybeans

set columns=120
set lines=60
set cmdheight=1
set noswapfile
set nobackup
set guifont=Ricty\ for\ Powerline:h15
set guioptions-=r
set guioptions-=L
set textwidth=0
set undodir=/tmp
set showtabline=2
set guioptions-=e
set clipboard=unnamed

set list
set listchars=tab:▸\ ,eol:¬

highlight Normal guibg=#232323
highlight NonText guifg=#585858 guibg=#2A2A2A
highlight SpecialKey guifg=#585858 guibg=#2A2A2A
